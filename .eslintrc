{
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "plugin:jsx-a11y/strict",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "plugins": ["jsx-a11y"],
  "ignorePatterns": ["scripts/template"],
  "rules": {
    "import/order": ["warn"],
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
