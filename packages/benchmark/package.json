{
  "name": "@coreym/benchmark",
  "description": "Benchmark Design System",
  "author": "Corey McGeachie",
  "license": "MIT",
  "version": "0.1.12",
  "private": false,
  "main": "index.js",
  "prepublish": "npm run release",
  "scripts": {
    "dev": "start-storybook -p 5000",
    "stats": "webpack --profile --json > webpack-stats.json",
    "watch": "webpack -w ./src/index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "clean": "rimraf dist public .cache",
    "build": "run-p build:*",
    "build:components": "webpack -p ./src/index.js",
    "build:storybook": "build-storybook -o dist/storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage",
    "dist:prepare": "run-s clean lint test build",
    "dist:copy": "copyfiles LICENSE README.md package.json dist/components",
    "dist:publish": "cd dist/components && npm publish --access public",
    "dist": "run-s dist:prepare dist:copy dist:publish"
  },
  "dependencies": {
    "@emotion/styled-base": "^10.0.31",
    "@reach/auto-id": "^0.6.1",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-roving-tabindex": "^0.7.1",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.6",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "prism-react-renderer": "^0.1.7",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
