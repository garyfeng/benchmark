{
  "name": "@coreym/benchmark",
  "description": "Benchmark Design System",
  "author": "Corey McGeachie",
  "license": "MIT",
  "version": "0.1.15",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "trailingComma": "none",
    "semi": true,
    "arrowParens": "avoid",
    "singleQuote": true
  },
  "main": "index.js",
  "scripts": {
    "stats": "webpack --profile --json > webpack-stats.json",
    "watch": "webpack -w ./index.js",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack -p ./index.js",
    "dist": "npm run dist:build && npm run dist:copy && npm run dist:publish",
    "dist:build": "npm run lint && npm run test && npm run build",
    "dist:copy": "copyfiles LICENSE README.md package.json dist",
    "dist:publish": "cd dist && npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@emotion/styled-base": "^10.0.31",
    "@reach/auto-id": "^0.6.1",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "prop-types": "^15.7.2",
    "react-roving-tabindex": "^0.7.1",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "@emotion/core": "10.x",
    "@emotion/styled": "10.x",
    "emotion-theming": "10.x",
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.6",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "emotion-theming": "^10.0.27",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "file-loader": "^4.2.0",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "jest-axe": "^3.3.0",
    "peer-deps-externals-webpack-plugin": "^1.0.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}